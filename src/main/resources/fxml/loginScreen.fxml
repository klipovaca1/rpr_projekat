<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="80.0" minWidth="80.0" prefWidth="80.0" />
        <ColumnConstraints halignment="RIGHT" hgrow="NEVER" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" />
        <ColumnConstraints halignment="LEFT" hgrow="NEVER" maxWidth="180.0" minWidth="180.0" prefWidth="180.0" />
        <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="80.0" minWidth="80.0" prefWidth="80.0" />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints valignment="CENTER" vgrow="NEVER" />
        <RowConstraints valignment="CENTER" vgrow="NEVER" />
        <RowConstraints valignment="CENTER" vgrow="NEVER" />
        <RowConstraints valignment="CENTER" vgrow="NEVER" />
    </rowConstraints>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
    </padding>
    <Label text="Welcome to courtroom &quot;BUTIK&quot;" GridPane.columnSpan="4">
        <GridPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
        </GridPane.margin>
        <font>
            <Font size="22.0" />
        </font>
    </Label>
    <Label text="Name:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextField fx:id="nameField" GridPane.columnIndex="2" GridPane.rowIndex="1" />
    <Label text="Password:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <PasswordField fx:id="passwordField" GridPane.columnIndex="2" GridPane.rowIndex="2" />
    <Button fx:id="loginButton" defaultButton="true" mnemonicParsing="false" onAction="#loginUser" prefWidth="70.0" styleClass="buttonContrasts" text="Login" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3">
        <GridPane.margin>
            <Insets top="10.0" />
        </GridPane.margin>
    </Button>
</GridPane>